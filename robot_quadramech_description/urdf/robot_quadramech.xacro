<joint name="Revolucionar 1" type="continuous">
  <origin xyz="-0.0 0.088 0.97" rpy="0 0 0"/>
  <parent link="second_link"/>
  <child link="third_link"/>
  <axis xyz="-1.0 -0.0 0.0"/>
</joint>

<joint name="Revolucionar 2" type="continuous">
  <origin xyz="0.0 0.0 0.13" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="second_link"/>
  <axis xyz="0.0 0.0 1.0"/>
</joint>

<joint name="Revolucionar 3" type="continuous">
  <origin xyz="-0.093 0.712 -0.05" rpy="0 0 0"/>
  <parent link="third_link"/>
  <child link="fouth_link"/>
  <axis xyz="-1.0 -0.0 0.0"/>
</joint>

<joint name="Rígido 4" type="fixed">
  <origin xyz="0.2 0.0 -0.3" rpy="0 0 0"/>
  <parent link="fouth_link"/>
  <child link="fifth_link"/>
</joint>

<joint name="Deslizador 5" type="prismatic">
  <origin xyz="-0.8 0.7 -0.257" rpy="0 0 0"/>
  <parent link="fifth_link"/>
  <child link="sixth_link"/>
  <axis xyz="0.0 0.0 -1.0"/>
  <limit upper="0.2" lower="0.0" effort="100" velocity="100"/>
</joint>

</robot>
