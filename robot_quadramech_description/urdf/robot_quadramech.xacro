  <visual>
    <origin xyz="0.0 -0.088 -1.1" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://robot_quadramech_description/meshes/third_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.0 -0.088 -1.1" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://robot_quadramech_description/meshes/third_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="second_link">
  <inertial>
    <origin xyz="0.0 0.0 -0.13" rpy="0 0 0"/>
    <mass value="0.0"/>
    <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0 -0.0 -0.13" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://robot_quadramech_description/meshes/second_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
