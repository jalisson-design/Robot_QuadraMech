<link name="sixth_link">
  <inertial>
    <origin xyz="0.693 -1.5 -0.493" rpy="0 0 0"/>
    <mass value="0.0"/>
    <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.693 -1.5 -0.493" rpy="0 0 0"/>
    <geometry>
